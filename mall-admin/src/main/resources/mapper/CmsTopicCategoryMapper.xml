<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rystars.mall.admin.cms.topic.mapper.CmsTopicCategoryMapper">

    <resultMap id="baseResultMap" type="com.rystars.mall.admin.cms.topic.bean.CmsTopicCategory">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="name" column="name" jdbcType="VARCHAR"/>
        <result property="icon" column="icon" jdbcType="VARCHAR"/>
        <result property="subjectCount" column="subject_count" jdbcType="INTEGER"/>
        <result property="showStatus" column="show_status" jdbcType="INTEGER"/>
        <result property="sort" column="sort" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="baseColumn">
        id,name,icon,
        subject_count,show_status,sort
    </sql>

</mapper>
